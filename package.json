{
  "name": "wordle",
  "version": "0.1.0",
  "homepage": "https://100milliongold.github.io/wordle",
  "private": true,
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "axios": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hotkeys-hook": "latest",
    "react-modal": "latest",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest",
    "redux": "latest",
    "redux-persist": "latest",
    "swr": "latest",
    "tailwindcss": "latest",
    "twin.macro": "latest",
    "web-vitals": "latest"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "latest",
    "@emotion/babel-plugin-jsx-pragmatic": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-modal": "latest",
    "babel-plugin-macros": "latest",
    "eslint": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "gh-pages": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
    "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\""
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "rules": {}
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
